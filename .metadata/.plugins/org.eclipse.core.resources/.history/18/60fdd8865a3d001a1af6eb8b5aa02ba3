import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;

public class DFACheckBasic {
	static State currentState;

	/*
	 * DFA States as Enumeration
	 */
	public enum State {
		A, B, C, D, E, F, G, H, I, J;
	}

	public static void main(String[] args) throws IOException {
		String inputFile = args[0];
		BufferedReader bfReader = new BufferedReader(new FileReader(inputFile));
		try {
			String line;
			while ((line = bfReader.readLine()) != null) {
				// Check if DFA recognizes.
				testLine(line);
			}
			bfReader.close();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

	/*
	 * Process the String with the DFA.
	 */
	static boolean testLine(String line) {
		boolean recognized = false;
		currentState = State.A;
		char[] cArr = line.toCharArray();
		for (char c : cArr) {
			switch (currentState) {
			case A:
				if (c == '0') {
					currentState = State.B;
					System.out.println("Transitioned to state B.");
				} else if (c == '1') {
					currentState = State.C;
					System.out.println("Transitioned to state C.");
				}
				break;
			case B:
				if (c == '0') {
					System.out.println("State remains in B.");
				} else if (c == '1') {
					currentState = State.E;
					System.out.println("Transitioned to state E.");
				}
				break;
			case C:
				if (c == '0') {
					currentState = State.D;
					System.out.println("Transitioned to state D.");
				} else if (c == '1') {
					System.out.println("State remains in C.");
				}
				break;
			case D:
				if (c == '0') {
					currentState = State.B;
					System.out.println("Transitioned to state B.");
				} else if (c == '1') {
					currentState = State.G;
					System.out.println("Transitioned to state G.");
				}
				break;
			case E:
				if (c == '0') {
					currentState = State.F;
					System.out.println("Transitioned to state F.");
				} else if (c == '1') {
					currentState = State.C;
					System.out.println("Transitioned to state C.");
				}
				break;
			case F:
				if (c == '0') {
					currentState = State.B;
					System.out.println("Transitioned to state B.");
				} else if (c == '1') {
					currentState = State.I;
					System.out.println("Transitioned to state I.");
				}
				break;
			case G:
				if (c == '0') {
					currentState = State.H;
					System.out.println("Transitioned to state H.");
				} else if (c == '1') {
					currentState = State.C;
					System.out.println("Transitioned to state C.");
				}
				break;
			case H:
				if (c == '0') {
					currentState = State.B;
					System.out.println("Transitioned to state B.");
				} else if (c == '1') {
					currentState = State.J;
					System.out.println("Transitioned to state J.");
				}
				break;
			case I:
				if (c == '0') {
					currentState = State.J;
					System.out.println("Transitioned to state J.");
				} else if (c == '1') {
					currentState = State.C;
					System.out.println("Transitioned to state C.");
				}
				break;
			case J:
				if (c == '0' || c == '1') {
					System.out.println("State remains in J.");
				}
				break;
			default:
				System.out.println("Current character has no valid state.");
			}
		}
		
		if(currentState != State.J) {
			System.out.println(line + " accepted");
			return recognized = true;
		} else {
			System.out.println(line + " rejected");
			return recognized = false;
		}
	}

}
